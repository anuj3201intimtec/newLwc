<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_Check</fullName>
    <active>false</active>
    <errorConditionFormula>Age__c   &lt;  20</errorConditionFormula>
    <errorDisplayField>Age__c</errorDisplayField>
    <errorMessage>Age should be greater than 20 years</errorMessage>
</ValidationRule>
